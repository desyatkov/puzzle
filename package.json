{
	"name": "cli-puzzle",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && dist/cli.js",
		"pretest": "npm run build",
		"test": "xo && ava",
		"lint": "eslint . --ext .ts"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"ava": "^3.13.0",
		"blessed": "^0.1.81",
		"lodash": "^4.17.20",
		"meow": "^5.0.0",
		"react": "^16.13.1",
		"react-blessed": "^0.6.2"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/meow": "^5.0.0",
		"@types/react-blessed": "^0.3.2",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"eslint": "^7.11.0",
		"install": "^0.13.0",
		"npm": "^6.14.8",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
